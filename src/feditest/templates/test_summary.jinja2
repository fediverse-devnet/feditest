    <table class="summary">
       <thead>
            <tr>
                <th>Status</th>
                <th>Count</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>Passed</td>
                <td>{{ summary.n_passed }}</td>
            </tr>
            <tr>
                <td>Failed</td>
                <td>{{ summary.n_failed }}</td>
            </tr>
            <tr>
                <td>Skipped</td>
                <td>{{ summary.n_skipped }}</td>
            </tr>
            <tr>
                <td>Errors</td>
                <td>{{ summary.n_errored }}</td>
            </tr>
        </tbody>
        <tfoot>
            <tr>
                <td>Total</td>
                <td>{{ summary.n_total }}</td>
            </tr>
        </tfoot>
    </table>

    <table class="feditest summary">
       <thead>
            <tr>
                <th>Status</th>
                <th>Count</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <th>Passed</th>
{%- if summary.n_passed != 0 %}
                <td class="status passed">{{ summary.n_passed }}</td>
{%- else %}
                <td>0</td>
{%- endif %}
            </tr>
            <tr>
                <th>Failed</th>
                <td>
                    <table class="failed">
                        <thead>
                            <tr>
                                <td colspan="2" rowspan="2"></td>
                                <th colspan="5">Interoperability</th>
                            </tr>
                            <tr>
                                <th>Compromised</th>
                                <th>Degraded</th>
                                <th>Unaffected</th>
                                <th>Unknown</th>
                                <th>Total</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <th rowspan="5">Conformance</th>
                                <th>Must</th>
{%- for interop_level in feditest.InteropLevel %}
                                <td class="status must {{ interop_level.name.lower() }}">{{ summary.count_failures_for(feditest.SpecLevel.MUST, interop_level) }}</td>
{%- endfor %}
                                <td class="status must total">{{ summary.count_failures_for(feditest.SpecLevel.MUST, None) }}</td>
                            </tr>
{%- for spec_level in [ feditest.SpecLevel.SHOULD, feditest.SpecLevel.IMPLIED, feditest.SpecLevel.UNSPECIFIED ] %}
                            <tr>
                                <td>{{ spec_level.formatted_name() }}</td>
{%-     for interop_level in feditest.InteropLevel %}
                                <td class="status must {{ interop_level.name.lower() }}">{{ summary.count_failures_for(spec_level, interop_level) }}</td>
{%-     endfor %}
                                <td class="status must total">{{ summary.count_failures_for(spec_level, None) }}</td>
                            </tr>
{%- endfor %}
                            <tr>
                                <td>Total</td>
{%- for interop_level in feditest.InteropLevel %}
                                <td class="status {{ interop_level.name.lower() }} total">{{ summary.count_failures_for(None, interop_level) }}</td>
{%- endfor %}
                                <td class="status total">{{ summary.n_failed }}</td>
                            </tr>
                        </tbody>
                    </table>
                </td>
            </tr>
            <tr>
                <th>Skipped</th>
{%- if summary.n_skipped != 0 %}
                <td class="status skip">{{ summary.n_skipped }}</td>
{%- else %}
                <td>0</td>
{%- endif %}
            </tr>
            <tr>
                <th>Errors</th>
{%- if summary.n_errored != 0 %}
                <td class="status error">{{ summary.n_errored }}</td>
{%- else %}
                <td>0</td>
{%- endif %}
            </tr>
        </tbody>
        <tfoot>
            <tr>
                <th>Total</th>
                <td>{{ summary.n_total }}</td>
            </tr>
        </tfoot>
    </table>
